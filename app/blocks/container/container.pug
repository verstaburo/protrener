mixin container()
  .container&attributes(attributes)
    +grid
      +col(1).container__padding
      block
      +col(1).contaner__padding

mixin container-narrow()
  .container&attributes(attributes)
    +grid
      +col(4).container__padding
      block
      +col(4).contaner__padding

mixin container-title()
  .container&attributes(attributes)
    +grid
      +col(2).container__padding
      block
      +col(2).contaner__padding
