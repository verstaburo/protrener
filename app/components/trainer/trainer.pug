mixin trainer(ava, brand, price, name, surname)
  +b.A.trainer&attributes(attributes)
    +e.DIV.wall
      +e.brand
        +e.IMG(src=brand).image
    +e.DIV.describe
      +avatar(ava).__avatar
      +e.price!= price
      +e.fullname
        +e.SPAN.name!= name
        +e.SPAN.surname!= surname
